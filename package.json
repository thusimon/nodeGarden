{
  "name": "node-garden",
  "version": "1.0.0",
  "description": "a simple web app",
  "main": "index.js",
  "scripts": {
    "build:dev": "node web-server/tools/build.dev",
    "build:prod": "node web-server/tools/build.prod",
    "test": "env-cmd -f ./web-server/config/test.env jest --runInBand",
    "test:watch": "env-cmd -f ./web-server/config/test.env jest --watch --runInBand",
    "start": "npm run build:prod && node web-server/src/app",
    "start:dev": "npm run build:dev && env-cmd -f ./web-server/config/dev.env nodemon web-server/src/app -e js,hbs"
  },
  "keywords": [],
  "author": "Lu",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "axios": "^0.19.0",
    "bad-words": "^3.0.3",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "hbs": "^4.0.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.2",
    "multer": "^1.4.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "sharp": "^0.23.1",
    "socket.io": "^2.3.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-react": "^7.6.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "env-cmd": "^10.0.1",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "supertest": "^4.0.2",
    "webpack": "^4.41.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thusimon/nodeGarden.git"
  }
}
